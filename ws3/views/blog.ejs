<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Blog</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="css/style.css" rel="stylesheet">
    </head>
    <body>
        <% posts.forEach(function(post) { %>
            <div>
                <h2><%= post.title %></h2>
                <%= post.body %>
            </div>
        <% }) %>
    </body>
</html>